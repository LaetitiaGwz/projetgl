// Description:
//    Utilisation simple de la classe Math
//
// Resultats:
//   asin(0.0) = 0
//   asin(0.5) = 0.5 (environ)
//   asin(-2.0) : débordement arithmétique
//
// Un exemple de programme Deca utilisant la classe Math

#include "Math.decah"
{ 
	Math m ;
	m = new Math();
	println("asin(0.0) = ", m.asin(0.0));
	println("asin(0.5) = ", m.asin(0.5));
	println("asin(-2.0) = ", m.asin(-2.0));

}
