// Description:
//    Utilisation simple de la classe Math
//
// Resultats:
//    sin(0.0) = 0.00000e+00
//    sin(1.57) = 1 (environ)
//    sin(3.14) = 0 (environ)
//    sin(100.0) = -0.5 (environ)
//
// Un exemple de programme Deca utilisant la classe Math

#include "Math.decah"
{ 
	Math m ;
	m = new Math();
	println("sin(0.0) = ", m.sin(0.0));
	println("sin(1.57) = ", m.sin(1.57));
	println("sin(3.14) = ", m.sin(3.14));
	println("sin(100.0) = ", m.sin(100.0));
}
