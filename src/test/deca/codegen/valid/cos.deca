// Un exemple de programme Deca utilisant la classe Math

#include "Math.decah"
{ 
	Math m ;
	m = new Math();
	println("cos(0.0) = ", m.cos(0.0));
	println("cos(1.57) = ", m.cos(1.57));
	println("cos(3.14) = ", m.cos(3.14));
	println("cos(100.0) = ", m.cos(100.0));
}
