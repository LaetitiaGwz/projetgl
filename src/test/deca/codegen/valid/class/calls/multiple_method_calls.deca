class Calcul {
   int carre(int a){
        return a*a;
   }

   int moinsTrois(int a){
        return a - 3;
   }

   int grosseExpression(int a,int b,int c,int d){
        return a*(c + d) + b*(c - a % c) % 3;
   }
}

{
    Calcul c = new Calcul();
    int res = c.carre(c.moinsTrois(3) + c.grosseExpression(1,2,3,4)*c.carre(3-4+8));
    print(res);

}

