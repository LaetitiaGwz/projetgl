class Maths {
   int carre(int a){
        return a*a;
   }

   int moinsTrois(int a){
        return a - 3;
   }

   int grosseExpression(int a,int b,int c,int d){
        return a*(c + d) + b*(c - a % c) % 3;
   }
}

{
    Maths maths = new Maths();
    int res = maths.carre(maths.moinsTrois(3) + maths.grosseExpression(1,2,3,4)*maths.carre(3-4+8));
    print(res);

}

